//Function without parameter and without return type:-

#include<stdio.h>

void add()
{
    int num1=45,num2=67,sum;
    sum=num1+num2;
    printf("Addition is:%d",sum);
}
int main()
{
    add();
    
    return 0;
}


//Function with parameter and without return type:-

#include<stdio.h>

void add(int a,int b)
{
    int sum;
    sum=a+b;
    printf("Addition is:%d",sum);
}
int main()
{
    add(5,4);
    
    return 0;
}


//Function without parameter and with return type:-

#include<stdio.h>

int add()
{
    int sum,a=5,b=4;
    sum=a+b;
    return sum;
}
int main()
{
     printf("Addition is:%d",add());
    
    return 0;
}


//Function with parameter and with return type:-

#include<stdio.h>

int add(int a,int b)
{
    int sum;
    sum=a+b;
    return sum;
}
int main()
{
     printf("Addition is:%d",add(20,45));
    
    return 0;
}
