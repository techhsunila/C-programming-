// Print the array elements.
#include <stdio.h>

int main() 
{
    int arr[5]={10,20,30,40,50},i;
    
    printf("Array elements: ");
    
    for (i = 0; i < 5; i++) 
    {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
    
}


// Calculate the sum of array elements.
#include <stdio.h>

int main() 
{
    int arr[5]={10,20,30,40,50};
    int i,sum=0;
    
    for (i = 0; i < 5; i++) 
    {
        sum += arr[i];
    }
    printf("Sum of array elements: %d\n", sum);
    
    return 0;
}


//Find the maximum element in the array.
#include <stdio.h>

int main() 
{
    int arr[5]={10,20,30,40,50},i;
    int max = arr[0];
    
    for (i = 1; i < 5; i++) 
    {
        if (arr[i] > max) 
        {
            max = arr[i];
        }
    }
    printf("Maximum element: %d\n", max);

    return 0;
}


//Reverse the array.
#include <stdio.h>

int main() 
{
    int arr[5]={10,20,30,40,50},i;
    int temp;
    
    for (i = 0; i < 5 / 2; i++) 
    {
        temp = arr[i];
        arr[i] = arr[5 - i - 1];
        arr[5 - i - 1] = temp;
    }
    
    printf("Reversed array: ");
    for (i = 0; i < 5; i++) 
    {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
